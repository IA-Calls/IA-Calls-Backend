{
  "name": "ia-calls-backend",
  "version": "1.0.0",
  "description": "Backend para IA Calls - API REST con Node.js y Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "node scripts/build.js",
    "build:simple": "node scripts/simple-build.js",
    "build:normal": "node scripts/normal-build.js",
    "restore:dev": "node scripts/restore-dev.js",
    "build:prod": "npm run clean && npm run lint && npm run test && npm run optimize",
    "clean": "node -e \"const fs=require('fs');['dist','build'].forEach(d=>fs.existsSync(d)&&fs.rmSync(d,{recursive:true,force:true}));console.log('Directories cleaned')\"",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest",
    "optimize": "echo \"Optimizando para producci√≥n...\"",
    "prebuild": "npm run clean",
    "postbuild": "echo \"Build completado exitosamente\"",
    "migrate": "node scripts/migrate.js",
    "migrate:time": "node scripts/migrate_time_field.js",
    "migrate:groups": "node scripts/migrate_group_fields.js",
    "migrate:storage": "node scripts/migrate_storage.js",
    "test:expiration": "node test-user-expiration.js",
    "test:cors": "node test-cors-config.js",
    "test:cors-fix": "node test-cors-fix.js",
    "test:login-expiration": "node test-login-expiration.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^7.16.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5"
  }
}
